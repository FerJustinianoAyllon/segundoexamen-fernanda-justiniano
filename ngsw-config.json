{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/favicon.ico",
          "/manifest.webmanifest",
          "/src/app/home/home.html",
          "/src/app/layout/layout.html",
          "/**/*.css",
          "/**/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/src/app/home/*.png",
          "/src/app/layout/*.png"
        ]
      }
    }
  ]
}
